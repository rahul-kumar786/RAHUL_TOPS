\b\fs24 CSS :--\b0\fs22\par
\par
/* Style for the whole form container */\par
.contact-form-container \{\par
    width: 50%;\par
    margin: 0 auto;\par
    padding: 20px;\par
    background-color: #f4f4f4; /* Change the background color */\par
    border-radius: 8px;\par
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);\par
\}\par
\par
/* Form title style */\par
h2 \{\par
    text-align: center;\par
    color: #333;\par
\}\par
\par
/* General form field styling (common elements) */\par
.form-group \{\par
    margin-bottom: 15px; /* Add margin between fields */\par
\}\par
\par
/* Input fields styling */\par
input[type="text"], input[type="email"], textarea \{\par
    width: 100%;\par
    padding: 10px;\par
    margin: 5px 0; /* Add margin to form fields */\par
    border: 1px solid #ccc;\par
    border-radius: 4px;\par
    font-size: 16px;\par
\}\par
\par
/* Style for textarea */\par
textarea \{\par
    height: 150px;\par
    resize: vertical;\par
\}\par
\par
/* Submit button styling */\par
#submit-button \{\par
    width: 100%;\par
    padding: 10px;\par
    background-color: #4CAF50; /* Green button */\par
    color: white;\par
    font-size: 16px;\par
    border: none;\par
    border-radius: 4px;\par
    cursor: pointer;\par
    transition: background-color 0.3s ease;\par
\}\par
\par
/* Submit button hover effect */\par
#submit-button:hover \{\par
    background-color: #45a049; /* Darker green on hover */\par
\}\par
\par
/* Unique element styling using ID (submit button) */\par
#submit-button:focus \{\par
    outline: none; /* Remove outline when focused */\par
\}\par
\par
/* Responsive design for smaller screens */\par
@media screen and (max-width: 768px) \{\par
    .contact-form-container \{\par
        width: 80%;\par
    \}\par
\}\par
